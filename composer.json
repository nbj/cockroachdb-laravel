{
    "name": "nbj/cockroachdb-laravel",
    "description": "CockroachDB driver for Laravel 5.6",
    "keywords": ["cockroach", "cockroachdb", "laravel"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@cego.dk"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Nbj\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nbj\\Cockroach\\CockroachServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
